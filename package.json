    "@strapi/strapi": "^5.4.2",
    "@strapi/typescript-utils": "^5.4.2",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2",
    "@strapi/typescript-utils": "^5.4.2"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.4.2",
